{
    "name": "@app/renderer",
    "version": "0.15.0",
    "description": "Electron renderer process code.",
    "type": "module",
    "homepage": "https://github.com/yifanwww/template-electron-react#readme",
    "license": "MIT",
    "author": "yifanwww <yifanw1101@gmail.com> (https://github.com/yifanwww)",
    "bugs": {
        "url": "https://github.com/yifanwww/template-electron-react/issues"
    },
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/yifanwww/template-electron-react.git",
        "directory": "app/app-renderer"
    },
    "scripts": {
        "build": "vite build --base=./",
        "clean": "rimraf coverage node_modules/.vite",
        "dev": "vite dev",
        "format": "prettier --write \"**/*.{cjs,cts,css,html,js,jsx,mjs,mts,json,less,scss,ts,tsx,yaml,yml}\"",
        "lint": "stylelint \"**/*.{css,scss}\" && eslint .",
        "lint:fix": "stylelint \"**/*.{css,scss}\" --fix && eslint . --fix",
        "test": "unit-test",
        "test-full": "unit-test",
        "typecheck": "pnpm run typecheck:vite && pnpm run typecheck:src",
        "typecheck:src": "tsc --project tsconfig.json --noEmit --incremental false",
        "typecheck:vite": "tsc --project tsconfig.vite.json --noEmit --incremental false --composite false"
    },
    "dependencies": {
        "@app/common": "workspace:*",
        "clsx": "catalog:",
        "react": "catalog:",
        "react-dom": "catalog:",
        "react-router": "catalog:",
        "web-vitals": "catalog:"
    },
    "devDependencies": {
        "@app-config/eslint-config": "workspace:*",
        "@app-config/scripts": "workspace:*",
        "@app-config/tsconfigs": "workspace:*",
        "@jest/globals": "catalog:",
        "@testing-library/dom": "catalog:",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/react": "catalog:",
        "@testing-library/user-event": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "@vitejs/plugin-react": "catalog:",
        "eslint": "catalog:",
        "postcss": "catalog:",
        "prettier": "catalog:",
        "rimraf": "catalog:",
        "sass": "catalog:",
        "stylelint": "catalog:",
        "stylelint-config-recess-order": "catalog:",
        "stylelint-config-sass-guidelines": "catalog:",
        "stylelint-config-standard": "catalog:",
        "stylelint-config-standard-scss": "catalog:",
        "stylelint-order": "catalog:",
        "typescript": "catalog:",
        "typescript-plugin-css-modules": "catalog:",
        "vite": "catalog:",
        "vite-plugin-checker": "catalog:",
        "vite-tsconfig-paths": "catalog:"
    }
}
